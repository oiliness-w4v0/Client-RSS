  "scripts": {
    "dev": "npm-run-all2 -p dev:client dev:electron",
    "dev:client": "vite",
    "dev:electron": "bun run package-electron-code && bun run start",
    "build": "npm-run-all2 build:client build:electron",
    "build:client": "vite build",
    "build:electron": "bun run package-electron-code && bun run make",
    "dev2": "bun run ./src/build.ts && electron .",
    "package-electron-code": "bun run ./src/build.ts",
    "studio": "npx drizzle-kit studio",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "generate": "npx drizzle-kit generate",
    "migrate": "npx drizzle-kit migrate",
    "release": "npm version patch && git push && git push --tags"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
